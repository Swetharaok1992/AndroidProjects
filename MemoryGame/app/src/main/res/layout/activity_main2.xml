<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.swetharao.memorygame.Main2Activity">

    <TextView
        android:text="Memory:Relative"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView4"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:id="@+id/spinner"
        android:entries="@array/array_findOptions"
        android:gravity="center"
        android:textAlignment="center" />

    <ImageView
        app:srcCompat="@drawable/front"
        android:layout_below="@+id/spinner"
        android:layout_alignParentStart="false"
        android:id="@+id/iv1"
        android:layout_height="90dp"
        android:layout_alignStart="@+id/spinner"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_below="@+id/iv1"
        android:id="@+id/iv5"
        android:layout_alignStart="@+id/iv1"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignStart="@+id/iv2"
        android:id="@+id/iv6"
        android:layout_below="@+id/iv2"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:id="@+id/iv7"
        android:layout_below="@+id/iv3"
        android:layout_alignStart="@+id/iv3"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_below="@+id/iv5"
        android:layout_alignStart="@+id/iv5"
        android:id="@+id/iv9"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignTop="@+id/iv9"
        android:layout_alignStart="@+id/iv6"
        android:id="@+id/iv10"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignTop="@+id/iv10"
        android:layout_alignEnd="@+id/iv7"
        android:id="@+id/iv11"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:id="@+id/iv13"
        android:layout_alignStart="@+id/iv9"
        android:layout_alignParentBottom="false"
        android:layout_below="@+id/iv9"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignStart="@+id/iv10"
        android:id="@+id/iv14"
        android:layout_alignParentBottom="false"
        android:layout_below="@+id/iv10"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignStart="@+id/iv11"
        android:id="@+id/iv15"
        android:layout_alignParentBottom="false"
        android:layout_below="@+id/iv11"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_above="@+id/iv11"
        android:layout_alignStart="@+id/iv4"
        android:id="@+id/iv8"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_above="@+id/iv15"
        android:layout_alignEnd="@+id/iv8"
        android:id="@+id/iv12"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignBottom="@+id/iv15"
        android:layout_alignStart="@+id/iv12"
        android:id="@+id/iv16"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_below="@+id/iv13"
        android:layout_marginTop="24dp"
        android:id="@+id/iv17"
        android:layout_alignParentStart="true"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignTop="@+id/iv17"
        android:layout_alignStart="@+id/iv14"
        android:id="@+id/iv18"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignTop="@+id/iv18"
        android:layout_alignStart="@+id/iv15"
        android:id="@+id/iv19"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:layout_alignBottom="@+id/iv19"
        android:layout_alignStart="@+id/iv16"
        android:id="@+id/iv20"
        android:layout_width="70dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:id="@+id/iv4"
        android:layout_width="70dp"
        android:layout_below="@+id/spinner"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="7dp"
        android:visibility="invisible" />

    <ImageView
        android:layout_height="90dp"
        app:srcCompat="@drawable/front"
        android:id="@+id/iv3"
        android:layout_marginLeft="55dp"
        android:layout_width="70dp"
        android:layout_below="@+id/spinner"
        android:layout_toStartOf="@+id/iv8"
        android:layout_marginEnd="27dp"
        android:visibility="invisible" />

    <ImageView
        app:srcCompat="@drawable/front"
        android:id="@+id/iv2"
        android:layout_height="90dp"
        android:layout_width="70dp"
        android:layout_below="@+id/spinner"
        android:layout_toStartOf="@+id/iv7"
        android:layout_marginEnd="18dp"
        android:visibility="invisible" />

</RelativeLayout>
